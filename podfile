#ios8.0 已上
platform :ios , '8.0'

#忽略所有警告
inhibit_all_warnings!

#使用 framework
use_frameworks!

#针对RVT.xcodeproj这个工程的RVT target导入依赖库

xcodeproj 'RVT.xcodeproj'
target :RVT do
    pod 'GPUImage', '~> 0.1.7'
    pod 'AFNNetworking', '~> 3.4.8'
    pod 'Masonry', '~> 1.1.0'
    pod 'IQKeyboardManager', '~> 6.2.0'
    pod 'ReactiveObjC', '~> 3.1.0'
end
